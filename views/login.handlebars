<div class="container" id="greeting-page">
			<div class="row header">
				<div class="col s12 center-align" id="title">
				    <div id="landing-header">PhiloPet</div>
				</div>
			</div>
		</div>
		<div class="container" id="signin-box">
			<div class="row" id="sign-in">
				<div class="col s12 center-align col m6 offset-m3">
					<form action="/login" method="post">
						<div class="row">
							<br><br><br>
							<div class="input-field col s12 center-align">
							     <input id="existing-email" name="email" type="email" class="validate" required>
							    <label for="email">Email</label>
							</div>
						</div>
						<div class="row">
							<div class="input-field col s12 center-align">
							    <input id="existing-password" name="password" type="password" class="validate" required>
							    <label for="password">Password</label>
							</div>
						</div>
						<div class="row">
							<div class="input-field col s12 center-align">
							    <button class="waves-effect waves btn-large" type="submit" id="login_submit"> SUBMIT </button>
							</div>
						</div>
					</form>
				</div>

				<div class="col s12 center-align">
				    <span class="flow-text">
						<a href="/signup" id="sign-up-link">New? Click Here To Sign up!</a>
					</span>
				</div>
			</div>
		</div>